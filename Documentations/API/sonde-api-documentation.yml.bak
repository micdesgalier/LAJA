openapi: '3.0.3'
info:
    description: Api Documentation
    version: '1.0'
    title: Api Documentation
tags:
  - name: sondes-end-point
    description: Gestion des sondes
paths:
  /sondes:
    get:
      tags:
        - sondes-end-point
      description: Liste toutes les sondes
      operationId: listSondeUsingGET
      responses:
        '200':
          description: OK
          content:
            'application/json':
              schema:
                type: array
                items:
                  $ref: '#/components/schemas/Sonde'
        '401':
          description: Unauthorized
        '403':
          description: Forbidden
        '404':
          description: Not Found
      deprecated: false
    post:
      tags:
        - sondes-end-point
      summary: addSondeur
      operationId: addSondeurUsingPOST
      requestBody:
        content:
          'application/json':
            schema:
              $ref: '#/components/schemas/Sondeur'
      responses:
        '200':
          description: OK
        '201':
          description: Created
        '401':
          description: Unauthorized
        '403':
          description: Forbidden
        '404':
          description: Not Found
      deprecated: false
  /sondes/{id}:
    get:
      tags:
        - sondes-end-point
      summary: ListSonde
      operationId: ListSondeUsingGET
      parameters:
        - name: id_sonde
          in: path
          description: id
          required: true
          schema:
            type: integer
            format: int32
      responses:
        '200':
          description: OK
          content:
            'application/json':
              schema:
                $ref: '#/components/schemas/Sonde'
        '401':
          description: Unauthorized
        '403':
          description: Forbidden
        '404':
          description: Not Found
      deprecated: false
components:
  schemas:
    Sonde:
      type: object
      properties:
        login:
          type: string
        mot_de_passe:
          type: string
        id_sonde:
          type: integer
          format: int32
      title: Sonde